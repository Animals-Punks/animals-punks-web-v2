{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    "@emotion/babel-plugin",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "@/*": "*",
          "@common/*": "common/*",
          "@assets/*": "assets/*",
          "@pages/*": "pages/*",
          "@User/*": "domains/User/*"
        }
      }
    ]
  ]
}